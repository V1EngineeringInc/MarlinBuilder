
opt_set SPINDLE_LASER_ACTIVE_STATE    HIGH


opt_enable \
    LASER_FEATURE \
    LASER_POWER_INLINE \
    LASER_MOVE_POWER \
    LASER_MOVE_G0_OFF \
    LASER_MOVE_G0_OFF

opt_disable \
    SPINDLE_LASER_FREQUENCY \
    LASER_POWER_INLINE_TRAPEZOID

echo "- Configured for 32 bit laser settings" >> README.md
